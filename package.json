{
  "name": "ai-sdk-chat",
  "version": "0.1.0",
  "description": "AI SDK Chat module for building chat interfaces",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "lib/styles.css",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:lib": "tsc --project tsconfig.lib.json && tsc --project tsconfig.lib.json --module esnext --outDir dist --outExtension .js=.mjs",
    "start": "next start",
    "lint": "eslint",
    "prepublishOnly": "npm run lint && npm run build:lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
