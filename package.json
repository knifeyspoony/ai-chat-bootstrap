    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-plugin-storybook": "^9.1.2",
    "next": "15.5.0",
    "playwright": "^1.55.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "storybook": "^9.1.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
