{
  "name": "ai-sdk-chat-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for AI SDK Chat library and demo",
  "scripts": {
    "dev": "pnpm --parallel dev",
    "build": "pnpm -r build",
    "build:lib": "pnpm --filter ai-chat-bootstrap build",
    "build:demo": "pnpm --filter ai-chat-bootstrap-demo build",
    "lint": "eslint .",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}