{
  "name": "ai-chat-bootstrap-demo",
  "version": "0.5.9",
  "private": true,
  "description": "Demo application for ai-chat-bootstrap library",
  "scripts": {
    "dev": "concurrently -k -n lib,demo,mcp -c blue,green,magenta \"pnpm --filter ai-chat-bootstrap dev\" \"next dev --turbopack\" \"pnpm run mcp\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "mcp": "tsx mcp/server.ts",
    "clean": "rm -rf .next out"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^3.0.4",
    "@ai-sdk/react": "^2.0.18",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "ai": "^5.0.18",
    "ai-chat-bootstrap": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "@modelcontextprotocol/sdk": "^1.18.1",
    "nanoid": "^5.0.7",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-resizable-panels": "^3.0.5",
    "shiki": "^3.12.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@ai-sdk/azure": "^2.0.16",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tsx": "^4.19.3",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "postcss": "^8",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  }
