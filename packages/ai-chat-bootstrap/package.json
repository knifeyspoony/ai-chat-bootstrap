{
  "name": "ai-chat-bootstrap",
  "version": "0.3.2",
  "description": "React UI + hooks for building modern AI chat interfaces fast. Based on AISDK and AISDK Elements",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    },
    "./tokens.css": "./dist/tokens.css",
    "./tokens.primitives.css": "./dist/tokens.primitives.css",
    "./tokens.semantic.css": "./dist/tokens.semantic.css",
    "./tokens.dark.css": "./dist/tokens.dark.css",
    "./tailwind.preset": "./dist/tailwind.preset.mjs",
    "./tailwind.plugin": "./dist/tailwind.plugin.mjs",
    "./ai-chat.css": "./dist/ai-chat.css",
    "./tailwind.css": "./dist/ai-chat.css",
    "./variants": {
      "types": "./dist/variants/index.d.ts",
      "require": "./dist/variants/index.js",
      "import": "./dist/variants/index.mjs"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "ai-chat-bootstrap": "bin/cli.js"
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "node ./scripts/build-css.mjs && tsdown",
    "dev": "node ./scripts/build-css.mjs --watch & tsdown --watch",
    "lint": "eslint lib/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist/",
    "release": "pnpm typecheck && pnpm lint && pnpm build && pnpm publish --access public",
    "prepublishOnly": "pnpm lint && pnpm build",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/provider-utils": "^3.0.4",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.263.1",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.12.2",
    "streamdown": "^1.2.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.0.17",
    "zustand": "^5.0.8"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "ai": ">=5",
    "@ai-sdk/react": ">=2"
  },
  "devDependencies": {
    "@ai-sdk/azure": "^2.0.16",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^5.0.18",
    "@ai-sdk/react": "^2.0.18",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.0",
    "eslint": "^9",
    "playwright": "^1.55.0",
    "postcss": "^8.4.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwindcss": "^3.4.0",
    "tsc-alias": "^1.8.16",
    "tsdown": "^0.14.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knifeyspoony/ai-chat-bootstrap.git"
  },
  "keywords": [
    "ai",
    "chat",
    "sdk",
    "react",
    "nextjs"
  ],
  "author": "knifeyspoony",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knifeyspoony/ai-chat-bootstrap/issues"
  },
  "homepage": "https://github.com/knifeyspoony/ai-chat-bootstrap#readme",
  "sideEffects": [
    "dist/tokens.css",
    "dist/ai-chat.css"
  ],
  "publishConfig": {
    "access": "public"
  }
}